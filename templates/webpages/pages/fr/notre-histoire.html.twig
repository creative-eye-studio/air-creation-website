<!-- Cover Page -->
<section class="cover-page">
<div class="text">
<h1>Notre histoire</h1>
</div>

<div class="image"><img alt="Présentation de Air Création" src="../uploads/images/pages/histoire/bg-page.jpg" /></div>
</section>
<!-- Origines -->

<section class="chrono-swiper-1 chrono-swiper-container">
<h2>L'évolution de l'ULM depuis la nuit des temps</h2>

<div class="swiper-wrapper">{% for date in chronoOrigines %}
<div class="swiper-slide">
<figure class="image"><img alt="{{ date.chronologiedate }}" src="../uploads/images/chronologie/{{ date.chronologieimg }}" /></figure>

<div class="text">
<h3>{{ date.chronologiedate }}</h3>

<p>{{ date.chronologietext }}</p>
</div>
</div>
{% endfor %} {% for date in chronoPionniers %}

<div class="swiper-slide">
<figure class="image"><img alt="{{ date.chronologiedate }}" src="../uploads/images/chronologie/{{ date.chronologieimg }}" /></figure>

<div class="text">
<h3>{{ date.chronologiedate }}</h3>

<p>{{ date.chronologietext }}</p>
</div>
</div>
{% endfor %} {% for date in chronoActual %}

<div class="swiper-slide">
<figure class="image"><img alt="{{ date.chronologiedate }}" src="../uploads/images/chronologie/{{ date.chronologieimg }}" /></figure>

<div class="text">
<h3>{{ date.chronologiedate }}</h3>

<p>{{ date.chronologietext }}</p>
</div>
</div>
{% endfor %}</div>

<div class="swiper-scrollbar">&nbsp;</div>

<div class="swiper-button-next chrono-swiper-button-next">&nbsp;</div>

<div class="swiper-button-prev chrono-swiper-button-prev">&nbsp;</div>
</section>